{
  "name": "@alshdavid/mach",
  "version": "0.0.0-local",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alshdavid/mach.git"
  },
  "files": [
    "cmd",
    "lib",
    "types",
    "package.json",
    "postinstall.mjs",
    "tsconfig.json"
  ],
  "bin": {
    "mach": "bin.js",
    "mach-dev": "bin_dev.sh"
  },
  "imports": {
    "#bin": {
      "source": "./cmd/bin.ts",
      "default": "./cmd/bin.js"
    },
    "#worker": {
      "source": "./lib/worker.ts",
      "default": "./lib/worker.js"
    }
  },
  "exports": {
    "source": "./lib/index.ts",
    "types": "./lib/index.ts",
    "default": "./lib/index.js"
  },
  "optionalDependencies": {
    "@alshdavid/mach-linux-amd64": "0.0.0-local",
    "@alshdavid/mach-linux-arm64": "0.0.0-local",
    "@alshdavid/mach-macos-amd64": "0.0.0-local",
    "@alshdavid/mach-macos-arm64": "0.0.0-local",
    "@alshdavid/mach-windows-amd64": "0.0.0-local",
    "@alshdavid/mach-windows-arm64": "0.0.0-local"
  },
  "devDependencies": {
    "@types/node": "22.x.x",
    "typescript": "5.x.x",
    "@swc/cli": "*",
    "@swc/core": "*",
    "@swc/plugin-transform-imports": "*",
    "@alshdavid/mach-os-arch": "*"
  },
  "peerDependencies": {
    "@types/node": ">=20"
  }
}
