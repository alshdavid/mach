{
  "name": "@workspace/babel-config-js-multitarget",
  "version": "1.0.0",
  "modern": "dist/modern/index.js",
  "legacy": "dist/legacy/index.js",
  "targets": {
    "modern": {
      "engines": {
        "browsers": "Chrome 60"
      }
    },
    "legacy": {
      "engines": {
        "browsers": "ie 11"
      }
    }
