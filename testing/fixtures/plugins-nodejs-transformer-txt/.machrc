{
  "resolvers": [
    "nitropack:resolver"
  ],
  "transformers": {
    "*.{js,mjs,jsm,jsx,es6,cjs,ts,tsx}": [
      "nitropack:transformer/javascript"
    ],
    "*.txt": [
      "node:./plugins/transformer.mjs"
    ],
    "*.css": [
      "nitropack:transformer/css"
    ],
    "*.html": [
      "nitropack:transformer/html"
    ],
    "*.{svg,png,json,gif,woff2,woff,txt}": [
      "nitropack:transformer/drop"
    ]
  }
}
